PROCEDURE sorting (A: ARRAY_OF INTEGER)
VAR
    i,n,j : INTEGER
    numtoinsert : INTEGER
BEGIN
    FOR i FROM 1 TO A[n] DO
        i:= 1; i < A[n]; i++;
        numtoinsert= A[i]
          j=i-1;
    END_FOR

    WHILE ( j >= 0 && A[j] > numtoinsert) DO
        j=j+1
        A[j+1]= A[j]
    END_WHILE

    WHILE (j >= 0 && A[j] < numtoinsert) DO
         A[j+1]=numtoinsert
    END_WHILE
END